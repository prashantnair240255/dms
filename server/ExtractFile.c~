#include "sheader.h"
#include "methods.h"

char* ExtractFile(char szFilePath[][200])
{
	printf("In Extract File\n");
	static char szSFileName[50];
	char *pFile;
	int nLoop;
	printf("In Extract File2\n");
	bzero(szSFileName,sizeof(szSFileName));
	pFile = strrchr(szFilePath[0],'/');
	*pFile++;
	printf("In Extract File3\n");
	for(nLoop=0;pFile[nLoop]!='.';nLoop++){
		if(pFile[nLoop]!='\0')
			szSFileName[nLoop] = pFile[nLoop];
		else{
			printf("Out of extractfile()\n");
			return szSFileName;
		}
	}
}
